<?xml version="1.0"?>
<block>
  <name>Message Map</name>
  <key>phychic_message_map</key>
  <category>[phychic]</category>
  <import>import phychic</import>
  <make>phychic.MessageMap($function)</make>

  <param>
    <name>Function</name>
    <key>function</key>
    <type>raw</type>
  </param>

  <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

  <source>
    <name>out</name>
    <type>message</type>
    <optional>1</optional>
  </source>
</block>
